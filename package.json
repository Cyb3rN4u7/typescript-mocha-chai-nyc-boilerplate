{
  "name": "algorithms-and-data-structures",
  "version": "1.0.0",
  "description": "Having fun with Algorithms",
  "main": "out/src/app.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyb3rN4u7/Algorithms-and-Data-Structures.git"
  },
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "coverage": "nyc --reporter=html --reporter=text mocha",
    "build":"tsc -p tsconfig.json"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Achilles Moraites",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nyc": "^11.7.2",
    "source-map-support": "^0.5.5",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3"
  }
}
